{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "staging": {
      "channel": "staging",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "staging",
        "EXPO_NO_DOTENV": "1"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
