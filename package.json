{
  "name": "expo-sweb",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 expo start",
    "start:staging": "cross-env APP_ENV=staging npm run start",
    "start:production": "cross-env APP_ENV=production npm run start",
    "prebuild": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 expo prebuild",
    "prebuild:staging": "cross-env APP_ENV=staging npm run prebuild",
    "prebuild:production": "cross-env APP_ENV=production npm run prebuild",
    "android": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 expo run:android",
    "android:staging": "cross-env APP_ENV=staging npm run android",
    "android:production": "cross-env APP_ENV=production npm run android",
    "ios": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 expo run:ios",
    "ios:staging": "cross-env APP_ENV=staging npm run ios",
    "ios:production": "cross-env APP_ENV=production npm run ios",
    "web": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 expo start --web",
    "build:staging:ios": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform ios",
    "build:staging:android": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform android",
    "build:production:ios": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform ios",
    "build:production:android": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform android",
    "lint": "expo lint",
    "gen:i18n": "i18next-scanner --config i18next-scanner.config.js"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.11",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.8",
